{
  "version": 2,
  "functions": {
    "api/scrap.js": {
      "maxDuration": 10
    },
    "api/live.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/scrap",
      "destination": "/api/scrap.js"
    },
    {
      "source": "/api/live",
      "destination": "/api/live.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/scrap.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}