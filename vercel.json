{
  "version": 2,
  "functions": {
    "api/scrap.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/scrap",
      "dest": "/api/scrap.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/scrap.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}